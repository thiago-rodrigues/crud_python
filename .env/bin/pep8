#!/home/thiago/projetos/cruds/sis_python/.env/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'pep8==1.7.0','console_scripts','pep8'
__requires__ = 'pep8==1.7.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pep8==1.7.0', 'console_scripts', 'pep8')()
    )
